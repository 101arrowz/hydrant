{
  "name": "hydrant",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "targets": {
    "frontend": {
      "source": [
        "src/index.html",
        "src/privacy.html"
      ],
      "context": "browser",
      "distDir": "build"
    },
    "backend": {
      "source": [
        "src-server/index.ts",
        "src-server/dev.ts"
      ],
      "context": "node",
      "distDir": "server"
    }
  },
  "dependencies": {
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@ag-grid-community/client-side-row-model": "^28.1.0",
    "@ag-grid-community/core": "^28.1.0",
    "@ag-grid-community/react": "^28.1.0",
    "@chakra-ui/icons": "^2.0.6",
    "@chakra-ui/react": "^2.2.6",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fastify/static": "^6.11.2",
    "@fontsource/inter": "^4.5.12",
    "@fullcalendar/core": "^5.11.2",
    "@fullcalendar/interaction": "^5.11.2",
    "@fullcalendar/react": "^5.11.2",
    "@fullcalendar/timegrid": "^5.11.2",
    "@parcel/resolver-glob": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@react-oauth/google": "^0.2.6",
    "@types/dotenv-flow": "^3.3.1",
    "@types/gapi.client.calendar": "^3.0.10",
    "@types/node": "^18.18.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.7",
    "buffer": "^6.0.3",
    "dotenv-flow": "^3.3.0",
    "html-entities": "^2.3.3",
    "ics": "^2.41.0",
    "msgpack-lite": "^0.1.26",
    "nanoid": "^3.3.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-colorful": "^5.6.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "run-p -r start:*",
    "start:frontend": "parcel watch --target frontend --no-autoinstall",
    "start:backend": "parcel watch --target backend --no-hmr --no-cache --no-autoinstall",
    "build": "tsc && parcel build"
  },
  "alias": {
    "process": false,
    "buffer": false
  },
  "engines": {
    "node": "^18.18.0"
  }
}
